<header>    
    <nav>
        <h1><%= link_to "Rafflies", root_path %></h1>
        <ul>
            <% if current_user %>
            <li><%= link_to current_user.name, current_user %></li>
            <li><%= link_to 'Logout', logout_path, method: :delete %></li>
            <% else %>
            <li><%= link_to 'Login', login_path %></li>
            <li><%= link_to 'Sign up', sign_up_path %></li>
            <% end %>
        </ul>
    </nav>
</header>